generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [citext, pgcrypto]
}

model User {
  id          String @id @default(uuid())
  email       String @unique @db.Citext
  phoneNumber String @unique
  firstName   String
  lastName    String
}
